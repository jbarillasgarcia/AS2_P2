eAAA,gBAAA,CAAA,KAAA,EAAA,OAAA;AACA;;AACA,aAAA,EAAA;AACA,eAAA,gBAAA,CAAA,KAAA,EAAA,OAAA;AACA;;AACA,aAAA,EAAA;AACA,eAAA,gBAAA,CAAA,KAAA,EAAA,OAAA;AACA;AAfA;;AAkBA,WAAA,aAAA,GAAA,KAAA;AACA,KA1BA;AA2BA,IAAA,SA3BA,uBA2BA;AACA,aAAA,KAAA,MAAA,CAAA,GAAA,CAAA,KAAA,kBAAA,GAAA,QAAA,CAAA;AACA,KA7BA;AA8BA,IAAA,YA9BA,0BA8BA;AACA,aAAA,KAAA,MAAA,CAAA,GAAA,CAAA,KAAA,kBAAA,GAAA,WAAA,CAAA;AACA,KAhCA;AAiCA,IAAA,WAjCA,yBAiCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,CAAA,GAAA,CAAA,CAAA,KAAA,CAAA,SAAA,EAAA,EAAA,KAAA,CAAA,YAAA,EAAA,CAAA,EAAA,IAAA,CAAA,UAAA,SAAA,EAAA;AACA,sBAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA,IAAA,EAAA,EAAA;AACA,oBAAA,KAAA,CAAA,SAAA,GAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA;;AACA,wBAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA,IAAA,EAAA,EAAA;AACA,sBAAA,KAAA,CAAA,aAAA,GAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA;AACA,iBAPA,EAOA,KAPA,CAOA,UAAA,MAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,iBATA,CADA;;AAAA;AAYA,gBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KA9CA;AA+CA,IAAA,UA/CA,sBA+CA,EA/CA,EA+CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,kBAAA,2BAAA,EAAA,CAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,IAAA,CAAA,IAAA,IAAA,EAAA,EAAA;AACA,oBAAA,MAAA,CAAA,WAAA;;AAEA,oBAAA,MAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,mBAJA,MAIA;AACA,oBAAA,MAAA,CAAA,gBAAA,CAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,iBARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,iBAVA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KA3DA;AA4DA,IAAA,gBA5DA,4BA4DA,KA5DA,EA4DA,IA5DA,EA4DA;AACA,WAAA,oBAAA,CAAA,KAAA,GAAA,KAAA;AACA,WAAA,oBAAA,CAAA,KAAA,GAAA,KAAA;AACA,WAAA,oBAAA,CAAA,IAAA,GAAA,IAAA;AAEA,WAAA,oBAAA,CAAA,KAAA,GAAA,IAAA;AACA;AAlEA,GAnBA;AAuFA,EAAA,OAvFA,qBAuFA;AACA,SAAA,WAAA;AACA;AAzFA,CAAA","sourcesContent":["<template>\n    <div>\n        <AppBar :isLoaded=\"loaded\" @onopendrawer=\"$emit('onopendrawer')\"/>\n        <NewEditUserDialog :action=\"action\" :usuario=\"selectedUsuario\" :opened=\"newUserDialog\" :hardwareItems=\"hardwareItems\" @onclose=\"onclose\" @onsuccess=\"loadAllData()\"/>\n        <v-row v-if=\"loaded\">\n            <v-col v-for=\"user in usersData\" :key=\"user.idUsuario\" cols=\"12\" xs=\"12\" sm=\"6\" md=\"4\" lg=\"4\" xl=\"3\">\n                <v-card>\n                    <v-card-title>\n                        <v-btn icon style=\"position: absolute; right:0px; top:0px;\" @click=\"deleteUser(user.idUsuario)\"><v-icon>clear</v-icon></v-btn>\n                        {{user.nombre}}\n                    </v-card-title>\n                    <v-card-subtitle>{{user.puesto}} - {{user.departamentoEmpresa}}</v-card-subtitle>\n                    <v-card-text>\n                        <v-row class=\"pa-0 ma-0\">\n                            <v-col align-self=\"center\"># hardware asignado: </v-col>\n                            <v-col justify=\"end\"><v-chip>{{user.detalle.length}}</v-chip></v-col>\n                        </v-row>\n                    </v-card-text>\n                    <v-card-actions>\n                        <v-btn rounded text outlined small color=\"primary\" @click=\"onEdit(user)\">Ver Usuario</v-btn>\n                    </v-card-actions>\n                </v-card>\n            </v-col>\n            <v-btn fab fixed right bottom color=\"primary\" @click=\"action='add';newUserDialog=true;\">\n                <v-icon>\n                    add\n                </v-icon>\n            </v-btn>\n        </v-row>\n        <v-snackbar \n            v-model=\"notificationSnackbar.state\"\n            :color=\"notificationSnackbar.color\"\n            :timeout=\"notificationSnackbar.timeout\"\n            >\n            {{notificationSnackbar.text}}\n        </v-snackbar>\n    </div>\n</template>\n<script>\nimport AppBar from '../GlobalComponents/AppBar'\nimport NewEditUserDialog from './NewEditUserDialog'\n\nexport default {\n    components:{\n        AppBar,\n        NewEditUserDialog,\n    },\n    data:()=>({\n        usersData:[],\n        hardwareItems:[],\n        loaded:false,\n        newUserDialog:false,\n        action:\"\",\n        selectedUsuario:\"\",\n        notificationSnackbar:{\n            state:false,\n            text:\"\",\n            color:\"\",\n            timeout:3000\n        }\n    }),\n    methods:{\n        onEdit(usuario){\n            this.action=\"edit\";\n            this.newUserDialog=true;\n            this.selectedUsuario=usuario\n        },\n        onclose(responseCode,message){\n            switch(responseCode){\n                case 23:\n                    this.showNotification(\"green\",message);\n                break;\n                case 24:\n                    this.showNotification(\"green\",message);\n                break;\n                case 28:\n                    this.showNotification(\"red\",message);\n                break;\n                case 29:\n                    this.showNotification(\"red\",message);\n                break;\n                case 30:\n                    this.showNotification(\"red\",message);\n                break;\n            }\n\n            this.newUserDialog=false;\n        },\n        loadUsers(){\n            return this.$axios.get(this.$webserviceBaseURL+\"/Users\");\n        },\n        loadHardware(){\n            return this.$axios.get(this.$webserviceBaseURL+\"/Hardware\");\n        },\n        async loadAllData(){\n            await this.$axios.all([this.loadUsers(),this.loadHardware()]).then((responses)=>{\n                if(responses[0].data.code==21){\n                    this.usersData=responses[0].data.data;\n                    if(responses[1].data.code==11){\n                        this.hardwareItems=responses[1].data.data;\n                    }\n                }\n            }).catch((errors)=>{\n                console.log(errors);\n            });\n\n            this.loaded=true;\n        },\n        async deleteUser(id){\n            await this.$axios.delete(this.$webserviceBaseURL+`/Users/Delete/${id}`).then((response)=>{\n                if(response.data.code==22){\n                    this.loadAllData();\n\n                    this.showNotification(\"green\",response.data.message);\n                }else{\n                    this.showNotification(\"red\",response.data.message);\n                }\n            }).catch((error)=>{\n                console.log(error);\n            });\n        },\n        showNotification(color,text){\n            this.notificationSnackbar.state=false;\n            this.notificationSnackbar.color=color;\n            this.notificationSnackbar.text=text;\n            \n            this.notificationSnackbar.state=true;\n        }\n    },\n    created(){\n        this.loadAllData();\n    }\n}\n</script>\n<style scoped>\n\n</style>"],"sourceRoot":"src/components/Users"}]}